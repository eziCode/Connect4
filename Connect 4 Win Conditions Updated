import random

user_moves = []
cpu_moves = []
userTurn = False
cpuTurn = False
gameOverUser = False
gameOverCpu = False
first_move = input("Who goes first P/C? ")
trueVar = True
while trueVar:
    if first_move == 'P' or first_move == 'p' or first_move == 'Player':
        userTurn = True
        cpuTurn = False
        trueVar = False
    elif first_move == 'C' or first_move == 'c' or first_move == 'CPU':
        userTurn = False
        cpuTurn = True
        trueVar = False
    else:
        first_move = input("Who goes first P/C? ")

